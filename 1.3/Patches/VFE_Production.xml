<?xml version="1.0" encoding="utf-8"?>

<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Furniture Expanded - Medical Module</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="VFE_TableMachiningLarge"]/statBases/WorkTableWorkSpeedFactor</xpath>
                    <value>
                        <WorkTableWorkSpeedFactor>1.25</WorkTableWorkSpeedFactor>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="VFE_TableSmithyLarge"]/statBases/WorkTableWorkSpeedFactor</xpath>
                    <value>
                        <WorkTableWorkSpeedFactor>1.25</WorkTableWorkSpeedFactor>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="VFE_TableStoveLarge"]/statBases/WorkTableWorkSpeedFactor</xpath>
                    <value>
                        <WorkTableWorkSpeedFactor>1.25</WorkTableWorkSpeedFactor>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="VFE_TableTailorLarge"]/statBases/WorkTableWorkSpeedFactor</xpath>
                    <value>
                        <WorkTableWorkSpeedFactor>1.25</WorkTableWorkSpeedFactor>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="VFE_TableStonecutterElectric"]/statBases/WorkTableWorkSpeedFactor</xpath>
                    <value>
                        <WorkTableWorkSpeedFactor>1.25</WorkTableWorkSpeedFactor>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>
