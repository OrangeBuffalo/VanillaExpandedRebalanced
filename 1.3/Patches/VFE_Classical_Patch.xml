<?xml version="1.0" encoding="utf-8"?>

<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Factions Expanded - Classical</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="VFEC_LargePassiveCooler"]/comps/li[@Class="CompProperties_HeatPusher"]/heatPushMinTemperature</xpath>
                    <value>
                        <heatPushMinTemperature>12</heatPushMinTemperature>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>
