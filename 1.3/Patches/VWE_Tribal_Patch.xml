<?xml version="1.0" encoding="utf-8"?>

<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Weapons Expanded - Tribal</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>

                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="VWE_MeleeWeapon_Shiv"]/tools/li[label="blade"]/power</xpath>
                    <value>
                        <power>8</power>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/ThingDef[defName="VWE_MeleeWeapon_Shiv"]/tools/li[label="point"]/power</xpath>
                    <value>
                        <power>10</power>
                    </value>
                </li>

            </operations>
        </match>
    </Operation>
</Patch>